SRC_GITURL_BASE:=$(strip $(SRC_URL_BASE))/$(strip $(SRC_NAME_BASE))
SRC_PATH_BASE:=$(strip $(SRC_NAME_BASE))-src


SRC_NAMES:=$(filter-out %_BASE, $(filter SRC_NAME_%, $(.VARIABLES)))
MOD_NAMES:=$(patsubst SRC_NAME_%,%, $(SRC_NAMES))


include $(TOP)/configure/CONFIG_BASE
include $(TOP)/configure/CONFIG_MODULES
